<script>
		
            $(function () {
                $("#slider-range").slider({
                    range: true,
                    min: 0,
                    max: 100000,
                    values: [{{amountk}}, {{amountkd}}],
                    step: 1000,
                    slide: function (event, ui) {
                        $("#amount").val(ui.values[ 0 ]);
                        $("#amount1").val(ui.values[ 1 ]);
						
                    }
                });
                $("#amount").val($("#slider-range").slider("values", 0));
                $("#amount1").val($("#slider-range").slider("values", 1));
            });
			
        </script>
    </head>
    <body id="results" >

        <header>
           {% include 'ProjectBundle:Default:header.html.twig' %}
        </header>

        <main>
            <div class="selection-box">
			   
                <form name="form1" method="POST" id="formx" action="javascript:void(null);">

                <div class="selection-box_criterion">
                    <p><strong>Назначение:</strong></p>
                    <p> <input type="checkbox" {% if inputFielsd[0] is not empty %} checked {% endif %} name="prom" value="pr" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = ' start(); call(); ' ><span>Промышленные</span><br>
                        <input type="checkbox" {% if inputFielsd[1] is not empty %} checked {% endif %}name="office" value="off" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = 'start();call();'><span>Офисные</span><br>
                        <input type="checkbox" {% if inputFielsd[2] is not empty %} checked {% endif %}name="torg" value="tor" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = 'start();call();'><span>Торговые</span><br>
                        <input type="checkbox" {% if inputFielsd[3] is not empty %} checked {% endif %}name="street" value="str" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = 'start();call();'><span>Уличные</span><br>
                        <input type="checkbox" {% if inputFielsd[4] is not empty %} checked {% endif %}name="fito" value="fito" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = 'start();call();'><span>Фито</span><br>
                        <input type="checkbox" {% if inputFielsd[5] is not empty %} checked {% endif %}name="azs" value="azs" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = 'start();call();'><span>Для АЗС</span><br>
						<input type="checkbox" {% if inputFielsd[20] is not empty %} checked {% endif %}name="project" value="project" onClick="document.getElementById('loading').style.visibility =  'visible'" onchange = 'start();call();'><span>Прожекторы</span></p>
						
                </div>
                <div class="selection-box_criterion">
                    <p><strong>Мощность,Вт:</strong></p>
                    <p><input type="radio" {% if inputFielsd[6] is not empty %} checked {% endif %}name="power" value="144"  onchange = 'start(); pause(); call();'/><span>1-44</span><br>
                        <input type="radio" {% if inputFielsd[7] is not empty %} checked {% endif %}name="power" value="4589"  onchange = 'start(); pause(); call();'/><span>45-89</span><br>
                        <input type="radio" {% if inputFielsd[8] is not empty %} checked {% endif %}name="power" value="90199"  onchange = 'start(); pause(); call();'/><span>90-199</span><br>
                        <input type="radio" {% if inputFielsd[9] is not empty %} checked {% endif %}name="power" value="2001000"  onchange = 'start(); pause(); call();'/><span>200-1000</span><br></p>
                </div>

                <div class="selection-box_criterion">
                    <p><strong>Степень защиты:</strong></p>
                    <p><input type="radio" {% if inputFielsd[10] is not empty %} checked {% endif %} name="ip" value="50"  onchange = 'start(); pause(); call();'><span>До 50 IP</span><br>
                        <input type="radio"{% if inputFielsd[11] is not empty %} checked {% endif %} name="ip" value="5167"  onchange = 'start(); pause(); call();'><span>51-67 IP</span><br>
                        <input type="radio" {% if inputFielsd[12] is not empty %} checked {% endif %} name="ip" value="6888"  onchange = 'start(); pause(); call();'><span>68-88 IP</span><br>

                </div>
                <div class="selection-box_criterion">
                    <p><strong>Способ крепления:</strong></p>
                    <p><input type="radio" {% if inputFielsd[13] is not empty %} checked {% endif %}name="naznachenie" value="Встраиваемые" onchange = ' start(); pause(); call();'><span>Встраиваемые</span><br>
                        <input type="radio" {% if inputFielsd[14] is not empty %} checked {% endif %}name="naznachenie" value="Накладные"  onchange = 'start(); pause(); call();'><span>Накладные</span><br>
                        <input type="radio" {% if inputFielsd[15] is not empty %} checked {% endif %}name="naznachenie" value="Уличные(консольные)" onchange = 'start(); pause(); call();'><span>Уличные(консольные)</span><br>
                        <input type="radio" {% if inputFielsd[16] is not empty %} checked {% endif %}name="naznachenie" value="Подвесные"  onchange = 'start(); pause(); call();'><span>Подвесные</span><br>
                        <input type="radio" {% if inputFielsd[17] is not empty %} checked {% endif %}name="naznachenie" value="Потолочные" onchange = 'start(); pause(); call();'><span>Потолочные</span><br>
                        <input type="radio" {% if inputFielsd[18] is not empty %} checked {% endif %}name="naznachenie" value="Настенные" onchange = 'start(); pause(); call();'><span>Настенные</span><br>
                        <input type="radio" {% if inputFielsd[19] is not empty %} checked {% endif %}name="naznachenie" value="С датчиком движения"  onchange = 'start(); pause(); call();'><span>С датчиком движения</span></p>
                </div>
                <div class="selection-box_slider">
                    <p><strong>Световой поток, Лм:</strong></p>
                    <div id="slider-range"></div>
                    <p>
                        <input type="text" id="amount" name="amountk" readonly style=" width: 45px; float:left; border:0;  font-weight:bold;">
                        <label type="text" readonly style="width: 45px; float:left;border:0; margin-left:3px;  font-weight:bold; margin-top:-3px;" >-</label>
                        <input type="text" id="amount1" name="amountkd" readonly style="width: 45px; float:left; margin-left:-37px; border:0;  font-weight:bold;">
                      
				  <button class="selection-box_reset" onclick='pause(); call(); ' style="width: 30px; height: 20px; border-radius: 40px 10px; float:left;margin-top:-2px; ">  >></button>
					</p>
                </div>
				
				  <button class="selection-box_reset" onclick='pause(); selChk(""); begun(); start(); call(); 'style="margin-top:-3px; ">  Сбросить все</button>
                    <input type="text" readonly name="index" id="index" value="{{index}}" style="visibility:hidden; ">
                </form>
			  
				
				    
            </div>
            <div class="products" >
                <div class="title-bar">
					
                    <h1>Каталог продукции</h1>
                    <h2>{% if count is defined %}{{count}}{% endif %} товаров</h2>
                </div>
                
                <div id="loading" style=" visibility:hidden; "></div>
                
				{% if result is defined %}
                {% for fixture in result%}
                <a href="/fixtures/{{fixture.type}}/{{fixture.id}}"><div class="products_item">
                        <div class="products_item_bg">
						<img src="{{fixture.picture}}" width="115" height="80"  style="margin-top: 15px;"  > </img>
						</div>
                        <h1>{{fixture.name}}</h1>
                        <p>{{fixture.shortDesc|raw}}</p>
                    </div></a>
                {% endfor %}
                {% endif %}
				<button class="selection-box_reset" id="index_buttum" onclick='pause(); index(); call();'style="width:665px; margin-left:18px; margin-top:-3px; border-radius: 8px;">  Показать еще</button>
            </div>
			
        </main>

        <footer>
            <p>Общество с дополнительной ответственностью «ОЛДИН»<br>220024, г. Минск, пер. Стебенева, 9а<br>УНП 690031739</p>
            <img class="footer_logo" src="web/img/footer_logo.png">
            <p class="copyrights">2016 &copy; Все права защищены.</p>
        </footer>
<script>
function start() {
document.getElementById('index').value = 7;
}

function begun() {
document.getElementById('amount').value = 0;
document.getElementById('amount1').value = 100000;
}
if({{knopka}}==1){
document.getElementById('index_buttum').style.visibility = 'hidden';
}
function index() {
 
$a=document.getElementById('index').value; 
$rez=Number($a)+7; 
document.getElementById('index').value = $rez;

}

</script>
